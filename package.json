		"mock",
		"network",
		"socket",
		"tcp",
		"test",
		"webmock"
	],
	"homepage": "https://github.com/framp/node-mitm",
	"bugs": "https://github.com/framp/node-mitm/issues",

	"author": {
		"name": "Federico Rampazzo",
		"email": "frampone@gmail.com",
		"url": "http://framp.me"
	},

	"repository": {
		"type": "git",
		"url": "git://github.com/framp/node-mitm.git"
	},

	"licenses": [{
		"type": "LAGPL",
		"url": "https://github.com/framp/node-mitm/blob/master/LICENSE"
	}],

	"main": "index.js",
	"scripts": {"test": "make test"},

	"dependencies": {
		"underscore": ">= 1.1.6 < 1.6"
	},

	"devDependencies": {
		"mocha": ">= 1.12.0 < 1.19",
		"must": ">= 0.13 < 0.14",
